{
    "name": "abelbm/laravel-theme",
    "description": "This package was created due to the delay of the update to allow compatibility with Laravel 5.4",
    "keywords": ["laravel", "theme", "layout", "asset", "widget", "twig"],
    "homepage": "https://github.com/abelbm/laravel-theme",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abel Bolanos Martinez",
            "email": "abelbmartinez@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "twig/twig": "1.x"
    },
    "require-dev": {
        "illuminate/support": ">=5.0.1"
    },
    "autoload": {
        "psr-4": {
            "Teepluss\\Theme\\": "src/"
        },
        "classmap": [
            "src/Exceptions.php"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev"
}
